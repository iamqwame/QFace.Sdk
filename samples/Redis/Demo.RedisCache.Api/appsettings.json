{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "QFace.Sdk.RedisCache": "Debug"
    }
  },
  "AllowedHosts": "*",
  "RedisCache": {
    "Provider": "Upstash",
    "KeyPrefix": "demo:cache:",
    "DefaultExpiration": "00:30:00",
    "ConnectionTimeout": 30000,
    "StackExchange": {
      "ConnectionString": "localhost:6379",
      "Database": 0,
      "ConnectTimeout": 5000,
      "SyncTimeout": 5000,
      "AbortOnConnectFail": false
    },
    "Upstash": {
      "Url": "",
      "Token": "",
      "RetryCount": 3,
      "RetryBaseDelayMs": 1000
    }
  }
}

